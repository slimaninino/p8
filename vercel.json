{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/noahswitch-website/browser",
  "framework": "angular",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
